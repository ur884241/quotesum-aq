{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/gematria.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/api/gematria",
      "dest": "api/gematria.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}